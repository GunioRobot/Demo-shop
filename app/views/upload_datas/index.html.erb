<div id="image-list">
  <h1>Listing images</h1>

  <table>
    <% @upload_datas.each do |upload_data| %>
      <tr class="<%= cycle('list-line-odd','list-line-even')%>">
        <td>
          <%= image_tag upload_data.image.url(:thumb)  %>
        </td>
        <td class="list-description">
          <dl>
            <dt><%= upload_data.image.url %></dt>
          </dl>
        </td>
        <td class="list-actions">
          <%= link_to 'Show', upload_data %></br>
          <%= link_to 'Edit', edit_upload_data_path(upload_data) %></br>
          <td><%= link_to 'Destroy', upload_data, :confirm => 'Are you sure?', :method => :delete %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
<br />
<%= link_to 'New image', new_upload_data_path %>
